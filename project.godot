config_version=5

[application]

config/name="IsDisAnotherTowerDefense"
run/main_scene="res://Scenes/Menus/Main_Menu.tscn"
config/features=PackedStringArray("4.5", "Mobile")
config/icon="res://icon.svg"

[autoload]

GameManager="*res://Scripts/Managers/GameManager.gd"
DebugHelper="*res://Scripts/Systems/DebugHelper.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/stretch/mode="canvas_items"

[file_customization]

folder_colors={
"res://Assets/": "red",
"res://Scenes/": "orange",
"res://Scripts/": "yellow"
}

[rendering]

renderer/rendering_method="mobile"
